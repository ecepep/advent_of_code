    "version": "0.2.0",
    "configurations": [
        {
            "name": "rust (Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${fileDirname}/../target/debug/rust_pkg.exe",
            "args": [ ],
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "cwd": "${fileDirname}/../",
            "preLaunchTask": "build"
        },
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}